  "author": "Vicente Victor Sombo",
  "license": "MIT",
  "dependencies": {
    "busboy": "^1.6.0",
    "cookie": "^0.5.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "sinon": "^15.2.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "mocha": "^10.2.0"
  }
}