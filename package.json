{
  "name": "lumosjs",
  "version": "1.0.0",
  "description": "nodejs framework",
  "main": "index.js",
  "imports": {
    "#service/*": "./app/services/*.js",
    "#controller/*": "./app/controllers/*.js",
    "#middleware/*": "./app/middlewares/*.js",
    "#model/*": "./app/models/*.js",
    "#server/*": "./lib/core/*.js",
    "#routes/*": "./routes/*.js",
    "#routeGroup": "./lib/utils/routeGroup.js",
    "#core": "./lib/core/handlers.js",
    "#core/*": "./lib/core/*.js",
    "#utils/*": "./lib/utils/*.js",
    "#status": "./lib/core/statusCode.js",
    "#validation/*": "./lib/utils/validation/*.js",
    "#helpers/*": "./lib/helpers/*.js",
    "#header/*": "./lib/core/*.js",
    "#app": "./lib/index.js",
    "#config/*": "./config/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lumosjs/lumos"
  },
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "test": "mocha"
